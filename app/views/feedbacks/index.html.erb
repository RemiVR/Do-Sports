<div class="row">
	<div class="col-md-4 pull-left">
		<ul class="nav nav-pills">
		  <li role="presentation"><%= link_to("Add New Feedback", new_feedback_path)%></li>
		</ul>
	</div>
</div>
<div class="feedback">
	<% @feedback.each do |feedback|%>
		<div class="fb-items">
	    	<p class="bg-info lead fb-name"><%= feedback.name%></p>
	    		<p class="fb-body"><%= feedback.body%></p>
		</div>
	<% end %>
</div>

